<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Online Result System</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<error-page>
		<error-code>404</error-code>

		<location>/jsp/ErrorView404.jsp</location>
	</error-page>
	<error-page>
		<error-code>500</error-code>

		<location>/jsp/ErrorView.jsp</location>
	</error-page>
</web-app>
